{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "https://localhost:5393"
      }
    }
  },
  "Commands": {
    "CHANGE_DIR": "cd",
    "EXECUTE_WEB_API": "for /f \"delims=\" %i in ('dir /s /p /b \"API[trial].csproj\"') do dotnet run --project \"%i\" -c release",
    "TASKKILL_NAME": "taskkill /IM"
  },
  "Projects": {
    "WEB_API_PATH": "bin\\Release\\net8.0\\API[trial].exe",
    "WEB_API_EXE": "API[trial].exe"
  }
}
